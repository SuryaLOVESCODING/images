window.addEventListener('load', function () {
    //replace default search text
    var searchboxes = document.querySelectorAll("div.main-head__wrap .gsc-search-box.gsc-search-box-tools .gsc-input");
    for (i = 0; i < searchboxes.length; i++) {
        searchboxes[i].placeholder = 'Search ACE';
    }
    //hide default search icon
    var searchbuttons = document.querySelectorAll(".gsc-search-button.gsc-search-button-v2");
    for (i = 0; i < searchbuttons.length; i++) {
        searchbuttons[i].setAttribute('src', '/images/trans.gif');
    }
});